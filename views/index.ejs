<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>AstroMe</title>
</head>

<body>
  <header>
    <h1>AstroMe</h1>
  </header>
  <main>
    <section>
      <% photos.forEach(photo=> { %>
        <div>
          <img src="<%= photo.imgUrl %>" alt="space photo">
          <h2><%= photo.title %></h2>
          <h3><%= photo.date %></h3>
          <p><%= photo.explanation %></p>
          <h4><%= photo.credit %></h4>
        </div>

      <% }) %>
    </section>
  </main>
  <footer></footer>
</body>

</html>